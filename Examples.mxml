<?xml version="1.0" encoding="utf-8"?>
<js:Application xmlns:fx="http://ns.adobe.com/mxml/2009"
				applicationComplete="application1_applicationCompleteHandler(event)"
				xmlns:js="library://ns.apache.org/royale/basic"
				>
		<fx:Style>
		@namespace js "library://ns.apache.org/royale/basic";

		js|ComboBoxList
		{
			IItemRenderer: ClassReference("itemrenderers.LabelAndIconRenderer");
		}
	</fx:Style>
	<fx:Script>
		<![CDATA[
			import org.apache.royale.events.Event;
			
			
			protected function application1_applicationCompleteHandler(event:Event):void
			{
				myDropDown.dataProvider = ["one", "two", "three"];
			}
			
		]]>
	</fx:Script>
	
	<js:valuesImpl>
		<js:SimpleCSSValuesImpl />
	</js:valuesImpl>	
	
	<js:initialView>
		<js:View>
			<js:ComboBox id="myDropDown"/>
		</js:View>
	</js:initialView>
</js:Application>
